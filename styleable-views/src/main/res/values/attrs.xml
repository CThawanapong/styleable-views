<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StyleableStrikeThroughTextView">
        <attr name="enableStrikeThrough" format="boolean" />
    </declare-styleable>

    <declare-styleable name="StyleableButton">
        <attr name="styleableButtonText" format="string" />
        <attr name="styleableButtonTextSize" format="dimension" />
        <attr name="styleableButtonBackgroundColor" format="color" />
        <attr name="styleableButtonTextColor" format="color" />
        <attr name="styleableButtonLeftDrawable" format="integer" />
        <attr name="styleableButtonDrawablePadding" format="dimension" />
        <attr name="styleableButtonHorizontalPadding" format="dimension" />
        <attr name="styleableButtonVerticalPadding" format="dimension" />
        <attr name="styleableButtonBorderColor" format="color" />
        <attr name="styleableButtonCornerRadius" format="dimension" />
        <attr name="styleableButtonElevation" format="dimension" />
    </declare-styleable>
</resources>